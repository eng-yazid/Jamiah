<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.yazid.jamiah">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MasterActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


        <activity android:name=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MasterActivity" />

        </activity>


        <activity android:name=".AddUsersActivity"

            android:parentActivityName=".MainActivity">


        </activity>

        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <activity android:name=".JamiahDetailsActivity"
            >

            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MasterActivity" />
        </activity>

        <activity
            android:name=".DetailActivity"
            android:parentActivityName=".MasterActivity">

            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MasterActivity" />

        </activity>
        <activity android:name=".login.SignInActivity" />
        <activity android:name=".login.SignupActivity"/>
    </application>

    <uses-permission android:name="android.permission.INTERNET" />
</manifest>